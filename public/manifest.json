{
    "name": "Jtab",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "a useless chrome tab page",
    "icons": {
      "16": "./static/icon-16.png",
      "128": "./static/icon-128.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "chrome://favicon/",
      "tabs",   
      "downloads",
      "bookmarks",
      "storage",
      "http://*/*",
      "https://*/*"
    ], 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" 
  }